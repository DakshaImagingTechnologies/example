8)Add channel
	<XMLDakshaImaging>
		<Action Name="AddChannel">
			<SessionID></SessionID>
			<ChannelName>anand</ChannelName>
			<UserName>admin</UserName>
			<Password>12345</Password>
			<IPAddress>192.168.1.204</IPAddress>
			<PortNo>5554</PortNo>
			<RTSP>mnkbjbn</RTSP>
			<RetrieveMode>1</RetrieveMode>
			<Status>1</Status>
			<StorageStatus>1</StorageStatus>
			<StorageSpace>2000</StorageSpace>
			<StorageFileType>H264</StorageFileType>
			<PTZ>0</PTZ>
			<ReplicaMode>0</ReplicaMode>
			<Make>xyz</Make>
			<Model>menn</Model>
			<Desc>hfshh</Desc>
			<StreamMode>1</StreamMode>
			<Motion>0</Motion>
			<DID>3018</DID>
			<AddChannelCGroup>
			<CGID>197784630</CGID>
			</AddChannelCGroup>
		</Action>
	</XMLDakshaImaging>

	Response
	
	<XMLDakshaImaging>
		<Response ActionName="AddChannel">
			<Error>
			<ErrorMsg>Failed to insert channelcgroup mapping into the Database</ErrorMsg>
			<ErrorId>103</ErrorId>
			</Error>
		</Response>
		<Response ActionName="AddChannel">
			<Success>ok</Success>
			<CID>197784634</CID>
		</Response>
	</XMLDakshaImaging>


